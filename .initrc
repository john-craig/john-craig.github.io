func() {
  sunday=$(date -d "-$(date +%u) day");
  file=$(date --date="${sunday} +1 day" +%m%d%Y)".md"

  echo $file

  for number in {1..5}
  do
    echo "**"$(date --date="${sunday} +${number} day" +%m/%d)'**<br/>\n' >> ./devlogs/$file;
    echo '\n</br></br>\n' >> ./devlogs/$file;
  done;
 }

alias newPost='func'


